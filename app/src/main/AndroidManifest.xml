<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ehappy.a8recipesofoctopus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logoocto_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logoocto_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OcTo1" android:parentActivityName=".MainActivity" />
        <activity android:name=".OcTo2" android:parentActivityName=".MainActivity" />
        <activity android:name=".OcTo3" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcTo4" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcTo5" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcTo6" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcTo7" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcTo8" android:parentActivityName=".MainActivity"/>
        <activity android:name=".OcHint" android:parentActivityName=".MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>